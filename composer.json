{
  "name": "opal/artisan-commands",
  "description": "Artisan Commands for Laravel",
  "keywords": ["laravel", "artisan", "commands"],
  "license": "MIT",
  "authors": [
    {
      "name": "David Smith",
      "email": "davidsmith@itsandbox.net"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/console": "~5.8|^6.0",
    "illuminate/filesystem": "~5.8|^6.0",
    "illuminate/support": "~5.8|^6.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Opal\\ArtisanCommands\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    },
    "laravel": {
      "providers": [
        "Opal\\ArtisanCommands\\OpalServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
